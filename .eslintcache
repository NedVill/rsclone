eturn null;\n  }\n\n  const deadline = exp.exp * 1000;\n\n  return Date.now() > deadline;\n};\n\nconst useAuth = () => {\n  const [token, setToken] = useState(null);\n  const [refreshToken, setRefreshToken] = useState(null);\n  const [ready, setReady] = useState(false);\n  const [userId, setUserId] = useState(null);\n  const [fullName, setUserName] = useState(null);\n\n  const login = useCallback((jwtToken, jwtRefreshToken, id, name) => {\n    setToken(jwtToken);\n    setRefreshToken(jwtRefreshToken);\n    setUserId(id);\n    setUserName(name);\n\n    localStorage.setItem(storageName, JSON.stringify(\n      {\n        token: jwtToken,\n        refreshToken: jwtRefreshToken,\n        userId: id,\n        fullName: name,\n      },\n    ));\n  }, []);\n\n  const logout = useCallback(() => {\n    setToken(null);\n    setUserId(null);\n    setRefreshToken(null);\n    setUserName(null);\n\n    localStorage.removeItem(storageName);\n    localStorage.removeItem(authToken);\n  }, []);\n\n  const getToken = useCallback(async () => {\n    try {\n      if (isExpired(token)) {\n        const data = JSON.parse(localStorage.getItem(storageName));\n\n        const url = 'https://rsclone-back-end.herokuapp.com/api/auth/refresh_token';\n\n        const headers = {};\n        headers['Content-Type'] = 'application/json';\n\n        const body = JSON.stringify({ refreshToken });\n\n        const updatedToken = await fetch(url, { method: 'POST', headers, body })\n          .then((newToken) => newToken.json());\n\n        setToken(updatedToken.token);\n\n        if (updatedToken.token && data?.userId && data?.fullName && data?.refreshToken) {\n          login(updatedToken.token, data.refreshToken, data.userId, data.fullName);\n        }\n\n        window.location.reload();\n      }\n    } catch (e) {\n      if (e.message === 'Session timed out,please login again!') {\n        logout();\n      }\n    }\n  }, [login, logout, token, refreshToken]);\n\n  useEffect(() => {\n    getToken();\n\n    const data = JSON.parse(localStorage.getItem(storageName));\n\n    if (data?.token && data?.userId && data?.fullName && data?.refreshToken) {\n      login(data.token, data.refreshToken, data.userId, data.fullName);\n    } else {\n      logout();\n    }\n\n    setReady(true);\n  }, [login, logout, getToken, setReady]);\n\n  return {\n    login,\n    logout,\n    token,\n    getToken,\n    fullName,\n    userId,\n    ready,\n  };\n};\n\nexport default useAuth;\n","/Users/new/WebDevelopment/Courses/rsclone/src/routes.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/theme/CustomThemeProvider.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/Header/Header.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/pages/HomePage.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/pages/AuthPage.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/pages/BoardPage.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/hooks/style.hook.js",[],"/Users/new/WebDevelopment/Courses/rsclone/src/variables.js",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/Aside/Aside.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/BoardCard/BoardCards.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/ValidationRequirements/ValidationRequirements.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/AuthCard/AuthCard.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/hooks/http.hook.js",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/AuthFormTabs/AuthFormTabs.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/AuthForm/AuthForm.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/BoardCard/BoardCard.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/CreateButton/CreateButton.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/utils/checkFieldValidity.jsx",[],"/Users/new/WebDevelopment/Courses/rsclone/src/components/BoardCreatePopup/BoardCreatePopup.jsx